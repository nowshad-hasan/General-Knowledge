<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.summithaque.generalknowledge.OtherFirst">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/img21"
        android:id="@+id/relativeLayout9"></RelativeLayout>

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/backOthOne"
        android:id="@+id/backOthOne"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#6699ff"
        android:onClick="backMethod"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/nextOthOne"
        android:id="@+id/nextOthOne"
        android:layout_alignTop="@+id/backOthOne"
        android:layout_alignRight="@+id/relativeLayout9"
        android:layout_alignEnd="@+id/relativeLayout9"
        android:background="#6699ff"
        android:onClick="nextMethod"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

</RelativeLayout>
