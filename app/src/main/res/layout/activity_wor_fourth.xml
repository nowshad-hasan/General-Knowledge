<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.summithaque.generalknowledge.WorFourth">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/img14"
        android:id="@+id/relativeLayout4"></RelativeLayout>

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/worFrtBack"
        android:id="@+id/worFrtBack"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="backMethod"
        android:background="#dbecf5"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/worFrtNext"
        android:id="@+id/worFrtNext"
        android:layout_alignBottom="@+id/worFrtBack"
        android:layout_alignRight="@+id/relativeLayout4"
        android:layout_alignEnd="@+id/relativeLayout4"
        android:onClick="nextMethod"
        android:background="#dbecf5"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

</RelativeLayout>
