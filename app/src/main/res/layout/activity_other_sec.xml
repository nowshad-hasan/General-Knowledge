<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.summithaque.generalknowledge.OtherSec">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/img22"
        android:id="@+id/relativeLayout10"></RelativeLayout>

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/backOthTwo"
        android:id="@+id/backOthTwo"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#94b2fe"
        android:onClick="backMethod"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="@string/nextOthTwo"
        android:id="@+id/nextOthTwo"
        android:layout_alignTop="@+id/backOthTwo"
        android:layout_alignRight="@+id/relativeLayout10"
        android:layout_alignEnd="@+id/relativeLayout10"
        android:background="#94b2fe"
        android:onClick="nextMethod"
        android:textColor="#123456"
        android:textSize="15sp"
        android:textStyle="bold|italic" />

</RelativeLayout>
